<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ismael.aula8softbluemenusactionbar" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActionBarActivity"
            android:label="@string/title_activity_1" />
        <activity
            android:name=".TestActivity"
            android:label="@string/title_activity_test"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ActionModeActivity"
            android:label="@string/title_activity_action_mode" >
        </activity>
        <activity android:name=".MultiActionModeActivity"
            android:label="@string/title_activity_multi_mode"/>
        <activity android:name=".PopupMenuActivity"
            android:label="@string/title_activity_popup"/>
    </application>

</manifest>
